"fixableWarningCount":0,"usedDeprecatedRules":"77"},"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/index.js",[],[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/App.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/serviceWorker.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/reducers/index.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/CartList.js",["192","193"],[],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport { useSelector } from 'react-redux'\nimport CartCard from './CartCard'\nimport { Button } from 'react-materialize'\nimport './CartList.scss'\n\nconst CartList = () => {\n  const { isLoading, items } = useSelector((state) => state.cart)\n\n  const [total, setTotal] = useState(0)\n\n  if (!isLoading) {\n    const addDecimals = (num) => {\n      return (Math.round(num * 100) / 100).toFixed(2)\n    }\n\n    items.price = addDecimals(\n      items.reduce((acc, item) => acc + item.price * item.quantity, 0)\n    )\n  }\n\n  let price = items.price\n\n  useEffect(() => {\n    setTotal(\n      (items.quantity = items.reduce((acc, item) => acc + item.quantity, 0))\n    )\n  }, [])\n\n  return (\n    <div className='cart-page'>\n      <div className='cart-ring-up'>\n        <h3> Total Items</h3>\n        <p>{total}</p>\n        <h3> Total Price</h3>\n        <p>${items.price}</p>\n        <Link\n          to={{\n            pathname: '/checkout',\n            state: { price },\n          }}\n        >\n          <Button>Checkout</Button>\n        </Link>\n      </div>\n      <div className='cart-list'>\n        <div className='cart-list-product'>\n          <h3>Your Shopping Cart</h3>\n          {items.map((product) => {\n            if (product.quantity !== 0) {\n              return (\n                <CartCard\n                  key={product.id}\n                  product={product}\n                  total={total}\n                  setTotal={setTotal}\n                />\n              )\n            }\n          })}\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default CartList\n\n// FOR FUN\n// MAKE CART VERY SECURE USING SHA256\n\n// PLAN\n// CREATE UNIQUE TOKEN FOR EACH CART ITEM WHEN ADDED TO CART\n// STORE TOKEN ON BE UNDER THAT USER\n// WHEN ACCESSING CART, CHECK TO SEE IF TOKEN IS AVAILABLE FOR EACH ITEM\n// IF NO DISPLAY IT, IF NOT, DO NOT DISPLAY\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/AddProduct.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/Banner.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/RequestProduct.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/Login.js",["194","195","196"],[],"import React, { useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport { useForm } from 'react-hook-form'\nimport { useHistory } from 'react-router-dom'\nimport { useSelector, useDispatch } from 'react-redux'\nimport { loginUser } from '../redux/actions/authActions'\nimport { getCart } from '../redux/actions/cartActions'\nimport undraw_Add_user from '../assets/undraw_Add_user.svg'\n\nconst Login = () => {\n  const { register, handleSubmit, errors } = useForm()\n  const dispatch = useDispatch()\n  const successfulLogin = useSelector((state) => state.isSuccess)\n  const history = useHistory()\n  console.log('Login error (useForm):', errors)\n\n  const onSubmit = (data) => {\n    dispatch(loginUser(data, history))\n    dispatch(getCart())\n    let path = `/`\n    history.push(path)\n  }\n\n  return (\n    <div style={{ maxWidth: '800px', margin: 'auto', padding: '20px' }}>\n      <h4>Login</h4>\n      <form onSubmit={handleSubmit(onSubmit)}>\n        <label>Username</label>\n        <input type='text' id='username' name='username' ref={register} />\n        <label>Password</label>\n        <input type='password' id='password' name='password' ref={register} />\n        <button class='btn waves-effect waves-light' type='submit'>\n          {' '}\n          Submit{' '}\n        </button>\n      </form>\n      <div>\n        <Link to='/register'>\n          <p>New to GoSaveMore?</p>\n          <button\n            style={{ marginBottom: '20px' }}\n            className='btn waves-effect waves-light'\n          >\n            Register\n          </button>\n        </Link>\n      </div>\n      <>\n        <img src={undraw_Add_user} />\n      </>\n    </div>\n  )\n}\n\nexport default Login\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/Register.js",["197"],[],"import React from 'react'\nimport { useForm } from 'react-hook-form'\nimport { useDispatch } from 'react-redux'\nimport { registerUser } from '../redux/actions/authActions'\nimport { useHistory } from 'react-router-dom'\n\nexport default function Register() {\n  const { register, handleSubmit, reset } = useForm()\n  const dispatch = useDispatch()\n  const history = useHistory()\n\n  const onSubmit = (data) => {\n    let updatedData = {\n      ...data,\n      isAdmin: 0,\n    }\n    dispatch(registerUser(updatedData))\n    let path = `/`\n    history.push(path)\n  }\n\n  return (\n    <div\n      style={{\n        maxWidth: '800px',\n        margin: 'auto',\n        marginBottom: '100px',\n        padding: '20px',\n        height: 'auto',\n      }}\n    >\n      <form onSubmit={handleSubmit(onSubmit)}>\n        <h4>Create account</h4>\n        <label>User Name</label>\n        <input\n          required\n          type='text'\n          placeholder='Username'\n          name='username'\n          ref={register}\n        />\n        <label>Email</label>\n        <input\n          required\n          type='email'\n          placeholder='Email'\n          name='email'\n          ref={register}\n        />\n        <label>Password</label>\n        <input\n          required\n          type='password'\n          placeholder='password'\n          name='password'\n          ref={register}\n        />\n        <label>First Name</label>\n        <input\n          required\n          type='text'\n          placeholder='First Name'\n          name='firstName'\n          ref={register}\n        />\n        <label>Last Name</label>\n        <input\n          required\n          type='text'\n          placeholder='Last Name'\n          name='lastName'\n          ref={register}\n        />\n\n        <button class='btn waves-effect waves-light' type='submit'>\n          Submit\n        </button>\n      </form>\n    </div>\n  )\n}\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/RequestSubmitted.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/Homepage.js",["198"],[],"import React from \"react\";\nimport ProductList from \"../components/ProductList\";\nimport undrawGiftBox from \"../assets/undraw_Gift_box.svg\";\n\nconst Homepage = ({ filteredProduct }) => {\n  return (\n    <div className=\"homepage\">\n      <div className=\"row\">\n        <ProductList filteredProduct={filteredProduct} />\n        <img src={undrawGiftBox}></img>\n      </div>\n    </div>\n  );\n};\n\nexport default Homepage;\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/Orders.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/Checkout.js",["199","200","201"],[],"import React, { useState, useEffect } from 'react'\nimport { useSelector } from 'react-redux'\nimport CheckOutForm from '../components/CheckOutForm'\n\nconst Checkout = (props) => {\n  const totalPrice = props.location.state.price\n\n  return (\n    <div\n      style={{\n        maxWidth: '800px',\n        margin: 'auto',\n        marginBottom: '100px',\n        padding: '20px',\n        height: 'auto',\n      }}\n    >\n      <h5>Checkout</h5>\n      <p>Total today is: {totalPrice}</p>\n      <CheckOutForm totalPrice={totalPrice} />\n    </div>\n  )\n}\n\nexport default Checkout\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/NavBar.js",["202"],[],"import React, { useState, useEffect } from 'react'\nimport './NavBar.scss'\nimport { useHistory } from 'react-router-dom'\nimport { useSelector, useDispatch } from 'react-redux'\nimport GoSaveMoreLogoHeader from '../assets/GoSaveMoreLogoHeader.png'\nimport SearchBar from '../components/SearchBar'\nimport { logOut } from '../redux/actions/authActions'\nimport { resetQuantity } from '../redux/actions/cartActions'\n\nimport { Navbar, NavItem, Icon, Badge } from 'react-materialize'\nimport { NavLink } from 'react-router-dom'\n\nconst NavBar = ({ placeholder, handleChange }) => {\n  const [user, setUser] = useState({\n    user: [],\n    isLoading: '',\n    isAuth: '',\n    isSuccess: '',\n  })\n\n  const [totalItems, setTotalItems] = useState(0)\n\n  const state = useSelector((state) => state.auth)\n  const localItemCount = useSelector((state) => state.cart.totalItems)\n\n  const dispatch = useDispatch()\n  const history = useHistory()\n\n  const handleLogOut = () => {\n    dispatch(resetQuantity())\n    dispatch(logOut(history))\n  }\n\n  useEffect(() => {\n    setUser(state)\n  }, [state, user])\n\n  useEffect(() => {\n    setTotalItems(localItemCount)\n  }, [localItemCount, user])\n\n  return (\n    <Navbar\n      className='nav-bar'\n      alignLinks='right'\n      id='mobile-nav'\n      menuIcon={<Icon>menu</Icon>}\n      brand={\n        <NavLink to='/'>\n          <img src={GoSaveMoreLogoHeader} id='logo' />\n        </NavLink>\n      }\n      options={{\n        draggable: true,\n        edge: 'left',\n        preventScrolling: true,\n      }}\n    >\n      <div className='nav-item'>\n        <div>\n          <NavItem className='nav-item-search'>\n            <SearchBar placeholder={placeholder} handleChange={handleChange} />\n          </NavItem>\n        </div>\n        {state.isAuth ? (\n          <div className='nav-item-auth'>\n            <NavLink className='navLink cart' to='/cart'>\n              Cart <Badge className='nav-item-cart-num'> {totalItems}</Badge>\n            </NavLink>\n\n            <NavLink className='navLink addProduct' to='/admin/addProducts'>\n              Add New\n            </NavLink>\n\n            <NavLink className='navLink requestProduct' to='/requestProduct'>\n              Request Product\n            </NavLink>\n\n            <NavLink className='navLink track' to='/tracking'>\n              Orders\n            </NavLink>\n\n            <NavLink className='signout' onClick={handleLogOut} to='#signout'>\n              Logout\n            </NavLink>\n          </div>\n        ) : (\n          <div className='nav-item-auth'>\n            <NavLink to='/cart'>\n              Cart\n              <Badge className='nav-item-cart-num'> {totalItems}</Badge>\n            </NavLink>\n\n            <NavLink className='login' to='/login'>\n              Login\n            </NavLink>\n\n            <NavLink className='register' to='/register'>\n              Register\n            </NavLink>\n\n            <NavLink className='addProduct' to='/admin/addProducts'>\n              Add New\n            </NavLink>\n\n            <NavLink className='navLink requestProduct' to='/requestProduct'>\n              Request Product\n            </NavLink>\n          </div>\n        )}\n      </div>\n    </Navbar>\n  )\n}\n\nexport default NavBar\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/Footer.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/pages/ProductInfo.js",["203","204"],[],"import React, { useState, useEffect } from 'react'\nimport { useSelector, useDispatch } from 'react-redux'\nimport { getProduct } from '../redux/actions/productsActions'\nimport { MediaBox, Button } from 'react-materialize'\nimport './ProductInfo.scss'\nimport { addToCart } from '../redux/actions/cartActions'\n\nconst ProductInfo = (props) => {\n  let id = props.match.params.id\n\n  const dispatch = useDispatch()\n  const item = useSelector((state) => state.products.item)\n\n  const handleClick = () => {\n    dispatch(addToCart(item))\n  }\n\n  useEffect(() => {\n    dispatch(getProduct(id))\n  }, [])\n\n  return (\n    <>\n      <div className='product-info-container'>\n        <MediaBox>\n          <img alt={item.productName} src={item.image} />\n        </MediaBox>\n        <div className='product-info-details'>\n          <h3>{item.productName}</h3>\n          <h4>${item.price}</h4>\n          <h4>Description: </h4>\n          <p>{item.description}</p>\n          <h4>Stocks: </h4>\n          <p>{item.stock}</p>\n          <h4>Ratings: </h4>\n          <p>{item.ratings}</p>\n          <Button\n            waves='light'\n            className='product-info-button'\n            onClick={handleClick}\n          >\n            Add\n          </Button>\n        </div>\n      </div>\n    </>\n  )\n}\n\nexport default ProductInfo\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/util/privateRoute.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/actions/productsActions.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/actions/cartActions.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/reducers/authReducer.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/reducers/productReducer.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/reducers/cartReducer.js",["205","206"],[],"import SaveForLater from \"../../components/SaveForLater\";\nimport { types } from \"../actions/index\";\nconst {\n  ADD_TO_CART,\n  INCREMENT_ITEM_QUANTITY,\n  DECREMENT_ITEM_QUANTITY,\n  RESET_ITEM_QUANTITY,\n  CART_SAVE_ITEM_START,\n  CART_SAVE_ITEM_SUCCESS,\n  CART_SAVE_ITEM_FAIL,\n  GET_CART_START,\n  GET_CART_SUCCESS,\n  GET_CART_FAIL,\n} = types;\nconst initialState = {\n  items: [],\n  isLoading: false,\n  err: \"\",\n  totalItems: 0,\n};\n\nconst cartReducer = (state = initialState, { type, payload }) => {\n  switch (type) {\n    case ADD_TO_CART:\n      // if cart is empty add to cart\n      let data = {\n        ...payload,\n        quantity: 1,\n      };\n\n      if (state.items.length === 0) {\n        return {\n          ...state,\n          items: state.items.concat(data),\n          totalItems: 1,\n        };\n      }\n      // if not equal then add item to cart\n      // if cart item id is equal to payload id just add quantity\n      let availableItem = state.items.find((item) => item.id === payload.id);\n      if (!availableItem) {\n        return {\n          ...state,\n          items: state.items.concat(data),\n          totalItems: (state.totalItems += 1),\n        };\n      } else {\n        state.items.filter((item) => {\n          if (item.id === payload.id) {\n            item.quantity += 1;\n          }\n        });\n\n        return {\n          ...state,\n          totalItems: (state.totalItems += 1),\n        };\n      }\n\n    case CART_SAVE_ITEM_START:\n      return {\n        ...state,\n        isLoading: true,\n      };\n    case CART_SAVE_ITEM_SUCCESS:\n      return {\n        ...state,\n        isLoading: false,\n      };\n    case CART_SAVE_ITEM_FAIL:\n      return {\n        ...state,\n        err: payload,\n      };\n\n    case GET_CART_START:\n      return {\n        ...state,\n        isLoading: true,\n      };\n\n    case GET_CART_SUCCESS:\n      // update the payload, if the payload is not in the cart items then push it to the items\n      // if not then just increase the quantity in the store.\n      let tallyTotalItems = 0;\n      payload.map((item) => (tallyTotalItems += item.quantity));\n\n      return {\n        ...state,\n        isLoading: false,\n        items: [...state.items, ...payload],\n        totalItems: tallyTotalItems,\n      };\n\n    case GET_CART_FAIL:\n      return {\n        ...state,\n        err: payload,\n      };\n\n    case INCREMENT_ITEM_QUANTITY:\n      for (let item of state.items) {\n        if (item.id === payload) {\n          item.quantity += 1;\n          state.totalItems += 1;\n        }\n      }\n      return state;\n\n    case DECREMENT_ITEM_QUANTITY:\n      for (let item of state.items) {\n        if (item.id === payload) {\n          item.quantity -= 1;\n          state.totalItems -= 1;\n        }\n\n        if (item.quantity === 0) {\n          return {\n            ...state,\n            items: state.items.filter((x) => x.id !== payload),\n          };\n        }\n      }\n      return state;\n\n    case RESET_ITEM_QUANTITY:\n      return {\n        items: [],\n        isLoading: false,\n        err: \"\",\n        totalItems: 0,\n      };\n\n    default:\n      return state;\n  }\n};\n\nexport default cartReducer;\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/ProductList.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/CheckOutForm.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/CartCard.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/actions/authActions.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/SearchBar.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/actions/index.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/util/axiosRoute.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/SaveForLater.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/BillingDetails.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/ProductCard.js",["207"],[],"import React, { useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { useDispatch } from \"react-redux\";\nimport { addToCart, addItemQuantity } from \"../redux/actions/cartActions\";\nimport { Card, CardTitle, Button } from \"react-materialize\";\nimport \"./ProductCard.scss\";\n\nconst ProductCard = (props) => {\n  const dispatch = useDispatch();\n  let product = props.product;\n\n  const { productName, price, image } = props.product;\n  const [isDisabled, setIsDisabled] = useState(false);\n\n  const handleClick = () => {\n    dispatch(addToCart(product));\n    dispatch(addItemQuantity());\n    setIsDisabled(true);\n  };\n\n  return (\n    <Card className=\"product\">\n      <CardTitle image={image} className=\"product-name\">\n        <Link to={`/product/${product.id}`}>\n          <h3>{product.productName}</h3>\n        </Link>\n      </CardTitle>\n      <p>${price}</p>\n      <Button\n        waves=\"light\"\n        onClick={handleClick}\n        className=\"product-button string\"\n        disabled={isDisabled}\n      >\n        {isDisabled ? <span>Added</span> : <span>Add</span>}\n      </Button>\n    </Card>\n  );\n};\n\nexport default ProductCard;\n","/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/CheckoutError.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/redux/util/axiosWithAuth.js",[],[],"/Users/lambda_school_loaner_18/Desktop/Sean/React Native/goSaveMore/goSaveMore-FE/gsm-frontend/src/components/FormField.js",[],[],{"ruleId":"208","severity":1,"message":"209","line":29,"column":6,"nodeType":"210","endLine":29,"endColumn":8,"suggestions":"211"},{"ruleId":"212","severity":1,"message":"213","line":50,"column":32,"nodeType":"214","messageId":"215","endLine":50,"endColumn":34},{"ruleId":"216","severity":1,"message":"217","line":1,"column":17,"nodeType":"218","messageId":"219","endLine":1,"endColumn":26},{"ruleId":"216","severity":1,"message":"220","line":13,"column":9,"nodeType":"218","messageId":"219","endLine":13,"endColumn":24},{"ruleId":"221","severity":1,"message":"222","line":49,"column":9,"nodeType":"223","endLine":49,"endColumn":38},{"ruleId":"216","severity":1,"message":"224","line":8,"column":35,"nodeType":"218","messageId":"219","endLine":8,"endColumn":40},{"ruleId":"221","severity":1,"message":"222","line":10,"column":9,"nodeType":"223","endLine":10,"endColumn":34},{"ruleId":"216","severity":1,"message":"225","line":1,"column":17,"nodeType":"218","messageId":"219","endLine":1,"endColumn":25},{"ruleId":"216","severity":1,"message":"217","line":1,"column":27,"nodeType":"218","messageId":"219","endLine":1,"endColumn":36},{"ruleId":"216","severity":1,"message":"226","line":2,"column":10,"nodeType":"218","messageId":"219","endLine":2,"endColumn":21},{"ruleId":"221","severity":1,"message":"222","line"